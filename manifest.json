{

    "manifest_version": 2,
    "name": "home-page",
    "version": "1.0",
    "icons": {
      "48": "node_modules/@fortawesome/fontawesome-free/svgs/regular/calendar-plus.svg",
      "96": "node_modules/@fortawesome/fontawesome-free/svgs/regular/calendar-plus.svg"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "http://localhost/*"
    ],
    "browser_action": {
        "default_icon": {
          "19": "node_modules/@fortawesome/fontawesome-free/svgs/regular/calendar-plus.svg",
          "48": "node_modules/@fortawesome/fontawesome-free/svgs/regular/calendar-plus.svg",
          "96": "node_modules/@fortawesome/fontawesome-free/svgs/regular/calendar-plus.svg"
        },
        "default_title": "Show my home"
      }
  }